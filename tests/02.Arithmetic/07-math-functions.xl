run_test x is
    writeln abs x
    writeln sqrt x
    // Avoid platform-dependent rounding errors for very small values
    writeln 0.25 + sin x
    writeln 0.25 + cos x
    writeln 0.25 + tan x
    writeln asin x
    writeln acos x
    writeln atan x
    writeln exp x
    writeln expm1 x
    writeln log x
    writeln log10 x
    writeln log2 x
    writeln log1p x
    writeln "---"

run_test 0.13331
run_test 1.3331
run_test pi
run_test 2.0*pi
